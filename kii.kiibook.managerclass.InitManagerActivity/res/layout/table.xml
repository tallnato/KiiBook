<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#606060"
    android:orientation="vertical" >

    <TableLayout
        android:id="@+id/table_body"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <TableRow
            android:id="@+id/table_titles"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/cell_shape"
                android:gravity="center"
                android:padding="5dp"
                android:text="Nome"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#fff" >
            </TextView>

            <TableLayout
                android:id="@+id/table_att"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Atitudes"
                        android:gravity="center"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                <TableRow
                    android:id="@+id/table_att_titles"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Assiduidade"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Pontualidade"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Comportamento"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Participação"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>
                </TableRow>
            </TableLayout>

            <TableLayout
                android:id="@+id/table_works"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Trabalhos"
                        android:gravity="center"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                <TableRow
                    android:id="@+id/table_att_titles"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Assiduidade"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Pontualidade"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Comportamento"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Participação"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>
                </TableRow>
            </TableLayout>

            <TableLayout
                android:id="@+id/table_att"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Testes"
                        android:gravity="center"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                <TableRow
                    android:id="@+id/table_att_titles"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Assiduidade"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Pontualidade"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Comportamento"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cell_shape"
                        android:padding="5dp"
                        android:text="Participação"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="#fff" >
                    </TextView>
                </TableRow>
            </TableLayout>

            <TextView
                android:id="@+id/textView1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/cell_shape"
                android:padding="5dp"
                android:text="Aval. Final"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#fff" >
            </TextView>
        </TableRow>
    </TableLayout>

</LinearLayout>