<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
  <objectAnimator 
    android:propertyName="y" 
    android:valueType="floatType"
    android:valueFrom="0"
    android:valueTo="800" 
    android:duration="@android:integer/config_mediumAnimTime"/>
